{
  "name": "squeak",
  "version": "1.0.0",
  "description": "Keep of track of all your favorite places",
  "main": "./server/index.ts",
  "scripts": {
    "watch": "nodemon ./server/index.ts",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node ./server/index.ts",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p ./",
    "build:frontend": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameygronewald/squeak.git"
  },
  "author": "Jamey Gronewald",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jameygronewald/squeak/issues"
  },
  "homepage": "https://github.com/jameygronewald/squeak#readme",
  "dependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.10.1",
    "@types/pg": "^7.14.7",
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
